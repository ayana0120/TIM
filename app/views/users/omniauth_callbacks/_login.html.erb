<div class="google">
  <%= link_to image_tag("btn_google_signin_dark_pressed_web.png"), user_google_oauth2_omniauth_authorize_path, method: :get %>
</div>

<div class="twitter">
  <%= link_to user_twitter_omniauth_authorize_path, method: :get, class:"twitter_log_in" do %>
    <%= image_tag("Twitter_Social_Icon_Square_Color.png",size:"46x46",class:"twitter_icon") %><b> Sign in with Twitter</b>
  <% end %>
</div>

<div class="yahoo">
  <%= link_to user_yahoojp_omniauth_authorize_path, method: :get do %>
    <img src="https://s.yimg.jp/images/login/btn/btnLLogin.gif" width="186" height="38"alt="Yahoo! JAPAN IDでログイン" border="0">
  <% end %>
</div>




